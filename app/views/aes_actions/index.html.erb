<h1>Listing aes_actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aes_actions.each do |aes_action| %>
      <tr>
        <td><%= link_to 'Show', aes_action %></td>
        <td><%= link_to 'Edit', edit_aes_action_path(aes_action) %></td>
        <td><%= link_to 'Destroy', aes_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aes action', new_aes_action_path %>
